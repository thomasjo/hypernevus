#!/usr/bin/env bash

set -eu

args="$@"

# Copy files from persistent storage to boost I/O performance.
echo "Copying data... "
rsync -avP --inplace --no-compress /storage/data/32 /data/32

# Execute command.
echo "Executing python with args"
echo "  ${args}"
python3 $args

# Copy local results to persistent storage.
# ...
